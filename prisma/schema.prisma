generator client {
  provider = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}
 
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
}
 
model User {
  id         Int      @id @default(autoincrement())
  firstName  String
  lastName   String
  username   String   @unique
  email      String   @unique
  createdAt  DateTime @default(now())
}

model Interest {
  id          Int      @id @default(autoincrement())
  name        String
  grade       Int
  skillLevel  String
  email       String?
  phoneNumber String?
  createdAt   DateTime @default(now())
}